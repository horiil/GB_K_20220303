package boj_test;

import java.util.Scanner;

public class A_plus_B_5_10952 {
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		label:
		int a = sc.nextInt();
		int b = sc.nextInt();

		while (a != 0 || b != 0) {
			System.out.println(a + b);
			
			break label;
		}
	}
}
