package boj_test;
import java.util.Scanner;

public class A_plus_B_5_10952 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int A, B = sc.nextInt();
		int C = 0;
		while( (A + B) != 0 ) {
			A = sc.nextInt();
			B = sc.nextInt();
			
			C = A + B;
			System.out.println(C);
		}
	}

}
