$(function() {

    $("#MOVE_TOP_BTN").click(function() {
        $('html, body').animate({
            scrollTop : 0
        }, 200);

        return false;
    });
    $("#MOVE_DN_BTN").click(function() {
          $('html, body').animate({
            scrollTop : windowHeight
        }, 200);

        return false;
    });

});